new Comparator<${1:Pair}> () {
	public int compare(${1:Pair} a, ${1:Pair} b) {
		if (a.x == b.x) {
			return Integer.compare(a.y, b.y);
		} else {
			return Integer.compare(a.x, b.x);
		}
	}
}
