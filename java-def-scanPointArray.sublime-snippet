static ${1:Point}[] scanPointArray(Scanner scanner, int n) {
	$1[] A = new $1[n];
	for (int i = 0; i < n; i++) {
		${2:int} x = scanner.next${2/.*/\u$0/}();
		${2:int} y = scanner.next${2/.*/\u$0/}();
		A[i] = new $1(x, y);
	}
	return A;
}
